#include<stdio.h>
#include<conio.h>

main()
{
	int arr[20];
	int i, c, n, komb;
	
	i=0;
	while (( c=getchar() ) != '\n' && i<20)
	{
		arr[i]=c-'0';
		++i;
	}
	n=i;
	
	komb=1;
	c=0;
	for (i=1; i<n-1; i++)
	{
		if (arr[i-1]+arr[i]==9 && arr[i]+arr[i+1]==9)
		{
			c=2;
			while (arr[i]+arr[i+1]==9)
			{
				c++;
				i++;
			}
			if (c%2!=0)
				komb++;
		}
	}
	
	printf("%d", komb);
	
	getch();
}
