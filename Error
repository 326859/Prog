#include <iostream>
using namespace std;
#include <conio.h>

int main(void)
{
    cout << "howboy! " << 8 << " Today" << endl << endl;

    cout << "in decimal: " << dec << 15 << endl;
    cout << "in oct: " << oct << 15 << endl;
    cout << "in hex: " << hex << 15 << endl;
    cout << "a floating-point number: " << 3.14159 <<endl;
    cout << "non-pointing char (escape): " << char(27) << endl << endl;

    cout << "This is far too long to put on a "
        "single line but it can be brocen up whiz "
        "no ill effects\nas long there is no "
        "punctuation separating adjacent character "
        "arrays.\n\n";

    cout << "dec to oct and hex" << endl;
    int number;
    cout << "Enter a decimal number: ";
    cin >> number;
    cout << "value in octal = 0" << oct << number
        << " value in hex = 0x" << hex << number << endl;

    getch();
    return(0);
}

