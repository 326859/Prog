#include <iostream>
#include <string.h>
using namespace std;

int main()
{
    string s0, s1, s2, s3, s4;
    string* sarr[5];

    sarr[0]=&s0;
    sarr[1]=&s1;
    sarr[2]=&s2;
    sarr[3]=&s3;
    sarr[4]=&s4;

    int c;

    while ( (c = getchar()) != '\n')
    {
        c = c-'0';
        if(c <= 4)
            *sarr[c] = *sarr[c] + char(c+'0');
        else
            *sarr[9-c] = *sarr[9-c] + char(9-c+'0');
    }

   if( s0.length() != 0 )
   {
       if (s1.length() != 0 )
       {
           int r=s1.length();
           s1 = "1" + s0;
           for (int i=1; i<r; i++)
               s1 = s1 + "1";
       }
       else if (s2.length() != 0 )
       {
           int r=s2.length();
           s2 = "2" + s0;
           for (int i=1; i<r; i++)
               s2 = s2 + "2";
       }
       else if (s3.length() != 0 )
       {
           int r=s3.length();
           s3 = "3" + s0;
           for (int i=1; i<r; i++)
               s3 = s3 + "3";
       }
       else if (s4.length() != 0 )
       {
           int r=s4.length();
           s4 = "4" + s0;
           for (int i=1; i<r; i++)
               s4 = s4 + "4";
       }
   }

    for (int i=1; i<5; i++)
     cout << *sarr[i];
}
